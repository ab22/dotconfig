{
    "catppuccin.italicKeywords": false,
    "catppuccin.italicComments": false,
    "editor.fontFamily": "'Liga SFMono Nerd Font', 'Iosevka Nerd Font', 'monospace', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 12,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "go.formatTool": "goimports",
    "go.useLanguageServer": true,
    "go.lintTool": "golangci-lint",
    "go.lintOnSave": "file",
    "go.toolsManagement.autoUpdate": true,
    "vim.cursorStylePerMode.insert": "line-thin",
    "vim.cursorStylePerMode.normal": "line-thin",
    "vim.cursorStylePerMode.replace": "block",
    "vim.cursorStylePerMode.visual": "line-thin",
    "vim.cursorStylePerMode.visualblock": "line-thin",
    "vim.cursorStylePerMode.visualline": "line-thin",
    "vim.useSystemClipboard": true,
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-b>": false,
        "<C-d>": true,
        "<C-f>": false,
        "<C-h>": false,
        "<C-i>": false,
        "<C-k>": false,
        "<C-p>": false,
        "<C-s>": false,
        "<C-z>": false
    },
    "workbench.colorTheme": "Catppuccin Mocha",
    "workbench.iconTheme": "catppuccin-mocha",
}
